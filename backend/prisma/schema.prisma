generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Team {
  id                    Int      @id @default(autoincrement())
  team_name            String
  leader_name          String
  state                String?
  institution          String?
  problem_statement    String?
  problem_identified   String?
  solution_description String?
  tech_stack           String?
  diagram_url          String?
  created_at           DateTime @default(now())
}
